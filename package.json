{
  "name": "roman-makes-music",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource/jockey-one": "^5.0.20",
    "@iconify-json/game-icons": "^1.1.9",
    "@iconify-json/gg": "^1.1.9",
    "astro": "^4.5.17",
    "astro-icon": "^1.1.0",
    "nanostores": "^0.10.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.4"
  },
  "devDependencies": {
    "@lamsal-de/eslint-config": "^2.0.21",
    "eslint": "8",
    "eslint-plugin-astro": "^0.34.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.13"
  },
  "eslintConfig": {
    "extends": [
      "@lamsal-de/eslint-config",
      "plugin:astro/base"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "overrides": [
      {
        "files": [
          "*.astro"
        ],
        "parser": "astro-eslint-parser",
        "parserOptions": {
          "parser": "@typescript-eslint/parser",
          "extraFileExtensions": [
            ".astro"
          ]
        },
        "rules": {}
      }
    ]
  }
}